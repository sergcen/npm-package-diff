{
    "name": "pkdiff",
    "version": "1.4.0",
    "description": "Diff npm packages across versions, ex. `pkdiff react 16.11.0`",
    "main": "src/index.js",
    "bin": {
        "pkdiff": "bin/pkdiff"
    },
    "homepage": "https://github.com/sergcen/npm-package-diff",
    "repository": {
        "type": "git",
        "url": "git@github.com:sergcen/npm-package-diff.git"
    },
    "files": [
        "bin/*",
        "src/*"
    ],
    "engines": {
        "node": ">=8.6.0"
    },
    "os": [
        "!win32"
    ],
    "scripts": {
        "test": "jest",
        "prepublishOnly": "prettier --loglevel=silent './src/**/*.js'"
    },
    "author": "Sergei Savelev (https://github.com/sergcen)",
    "license": "ISC",
    "dependencies": {
        "commander": "4.1.0",
        "diff2html": "3.0.0-master.128204d",
        "diff2html-cli": "4.2.1",
        "execa": "1.0.0",
        "micromatch": "4.0.2",
        "npm-package-arg": "8.0.0",
        "open": "7.0.0",
        "prettier": "1.19.1",
        "string-hash": "1.1.3"
    },
    "prettier": {
        "trailingComma": "all",
        "tabWidth": 4,
        "singleQuote": true
    },
    "devDependencies": {
        "jest": "25.1.0"
    }
}
